    <string-array name="import_policy_detail">
        <item>DUPLICAR - Adiciona um cópia dos registos que já existem.</item>
        <item>MANTER - Não substitui os registos existentes e ignorar os registos do CSV.</item>
        <item>SUBSTITUIR - Os registos no CSV substituem os registos existentes.</item>
        <item>RESTAURAR - Elimina os registos existentes e depois importa os registos do CSV.</item>
    </string-array>
    
