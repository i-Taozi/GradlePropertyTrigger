<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wefika.flowlayout.example" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/ExampleTheme">
        <activity
            android:name="com.wefika.flowlayout.example.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.wefika.flowlayout.example.BasicActivity"
            android:label="@string/activity_basic" />

        <activity
            android:name="com.wefika.flowlayout.example.AllInOneActivity"
            android:label="@string/activity_all_in_one" />

        <activity
            android:name=".VisibilityActivity"
            android:label="@string/activity_visibility" />

        <activity
            android:name=".ScrollActivity"
            android:label="@string/activity_scroll" />

    </application>

</manifest>
